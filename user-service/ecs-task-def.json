{
  "family": "service-a",
  "containerDefinitions": [
    {
      "name": "service-a",
      "image": "<your-ecr-url>/service-a:latest",
      "portMappings": [{ "containerPort": 8081 }],
      "memory": 512,
      "cpu": 256,
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "ecsTaskExecutionRole"
}
